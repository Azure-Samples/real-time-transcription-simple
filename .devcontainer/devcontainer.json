{
    "image": "mcr.microsoft.com/vscode/devcontainers/javascript-node:18",
    "hostRequirements": {
      "cpus": 4
    },
    // "waitFor": "onCreateCommand",
    "updateContentCommand": "cd webapp && yarn install",
    "postCreateCommand": "",
    "customizations": {
      "codespaces": {
        "openFiles": [
          "webapp/src/App.js"
        ]
      }
    },
    "portsAttributes": {
      "3000": {
        "label": "Application"
      }
    },
    "forwardPorts": [3000]
  }